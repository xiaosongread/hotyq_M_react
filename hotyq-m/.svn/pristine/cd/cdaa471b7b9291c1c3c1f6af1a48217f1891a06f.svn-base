@charset 'utf-8';
/*全局定义--------------------------------------------------------------------*/
body,html,div,ul,li,dl,dd,p,span,strong,h1,h2,h3,h4,h5,label,a{ 
    margin: 0px; padding: 0px; box-sizing: border-box; 
}
body, html,#root,.container{ 
	width: 100%;
    height: 100%; 
    position: relative;
}
body {
    font-size: 14px; 
    font-family: Microsoft YaHei, Helvetica, Helvetica Neue;
    color: #555; 
    background-color: #fff !important; 
    -webkit-font-smoothing: antialiased; 
    -webkit-text-size-adjust: none;  
    overflow-x: hidden;
}
html.pc body {
	background-color: #fff;
	margin: 0 auto;
	max-width: 750px
}

html.pc {
	background-color: #ccc;
}
ol,ul { 
    list-style: none;
}
a,a:link,a:visited,a:hover { 
    text-decoration: none; color: #555; 
}
a:focus{
    outline-width: 0px;
}
input,button,textarea { 
    border: 0; 
    border-radius: 0; 
    background: transparent; 
    -webkit-appearance: none; 
    -webkit-box-sizing: border-box; 
    box-sizing: border-box; 
    resize: none; 
    outline: 0; 
}
@font-face {
  font-family: 'iconfont';  /* project id 158149 */
  src: url('//at.alicdn.com/t/font_9ppx789mjg5bfbt9.eot');
  src: url('//at.alicdn.com/t/font_9ppx789mjg5bfbt9.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_9ppx789mjg5bfbt9.woff') format('woff'),
  url('//at.alicdn.com/t/font_9ppx789mjg5bfbt9.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_9ppx789mjg5bfbt9.svg#iconfont') format('svg');
}                
                    
.iconfont{
    font-family:"iconfont";
    font-size:16px;
    font-style:normal;
}

.swiper-container {
    width: 100%;
    height: 100%;
}